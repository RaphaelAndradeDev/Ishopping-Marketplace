@model Ishopping.ViewModels.TemplateProfessional.IndexEgretViewModelDeserialize

@{
    Layout = null;
    var i = 0; var j = 0; var cl = "";
}
<!DOCTYPE HTML>
<html lang="pt-br">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Html.Partial("_PartialMetaTags")

    <link rel="stylesheet" href="~/Componentes/Professional/Egret/css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/font-awesome.min.css">
    <link rel="stylesheet" href="~/Componentes/Professional/Egret/css/linea-icon.css" />
    <link rel="stylesheet" href="~/Componentes/Professional/Egret/css/fancy-buttons.css" />

    <!--=== Google Fonts ===-->
    <link href='http://fonts.googleapis.com/css?family=Bangers' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Roboto+Slab:300,700,400' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Raleway:600,400,300' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700' rel='stylesheet' type='text/css'>
    @if (!string.IsNullOrEmpty(Model.Profile.GoogleFonts))
    {
        <link href='@Model.Profile.GoogleFonts' rel='stylesheet' type='text/css'>
    }
    <!--=== Other CSS files ===-->
    <link rel="stylesheet" href="~/Componentes/Professional/Egret/css/animate.css" />
    <link rel="stylesheet" href="~/Componentes/Professional/Egret/css/jquery.vegas.css" />
    <link rel="stylesheet" href="~/Componentes/Professional/Egret/css/baraja.css" />
    <link rel="stylesheet" href="~/Componentes/Professional/Egret/css/jquery.bxslider.css" />

    <!--=== Main Stylesheets ===-->
    <link rel="stylesheet" href="~/Componentes/Professional/Egret/css/style.css" />
    <link rel="stylesheet" href="~/Componentes/Professional/Egret/css/responsive.css" />

    <!--=== Color Scheme, three colors are available red.css, orange.css and gray.css ===-->
    <link rel="stylesheet" href="~/Content/uploads/@Model.Profile.SiteNumber/gray.css" />
    <link href="~/Content/uploads/@Model.Profile.SiteNumber/iscss.css" rel="stylesheet">

</head>
<body>
    <!--=== Preloader section starts ===-->
    <section id="preloader">
        <div class="loading-circle fa-spin"></div>
    </section>
    <!--=== Preloader section Ends ===-->
    <!--=== Header section Starts ===-->
    <div id="header" class="header-section">
        <!-- sticky-bar Starts-->
        <div class="sticky-bar-wrap">
            <div class="sticky-section">
                <div id="topbar-hold" class="nav-hold container">
                    <nav class="navbar" role="navigation">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-responsive-collapse">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <!--=== Site Name ===-->
                            <a class="site-name navbar-brand" href="#">@Model.Profile.Empresa</a>
                        </div>

                        <!-- Main Navigation menu Starts -->
                        <div class="collapse navbar-collapse navbar-responsive-collapse">
                            <ul class="nav navbar-nav navbar-right">
                                @foreach (var menu in Model.Menu)
                                {
                                    <li class="current"><a href="@menu.ViewLink">@menu.TextMenu</a></li>
                                    foreach (var item in menu.UserMenuViewItem.Where(x => x.OnMenu == true))
                                    {
                                        <li><a href="@item.Link">@item.TextMenu</a></li>
                                    }
                                }
                            </ul>
                        </div>
                        <!-- Main Navigation menu ends-->
                    </nav>
                </div>
            </div>
        </div>
        <!-- sticky-bar Ends-->
        <!--=== Header section Ends ===-->
        <!--=== Home Section Starts ===-->
        <div id="section-home" class="home-section-wrap center">
            <div class="section-overlay"></div>
            <div class="container home">
                <div class="row">
                    <h1 class="well-come @Model.Textos[0].StText">@Html.Raw(Model.Textos[0].Text)</h1>

                    <div class="col-md-8 col-md-offset-2">
                        <p class="intro-message @Model.Textos[1].StText">@Html.Raw(Model.Textos[1].Text)</p>

                        <div class="home-buttons">
                            @if (Model.Buttons[0].TextButton != "")
                            {
                                <a href="@Model.Buttons[0].UrlButton" target="_blank" class="fancy-button button-line button-white vertical @Model.Buttons[0].StButton">
                                    @Html.Raw(Model.Buttons[0].TextButton)
                                    <span class="icon">
                                        <i class="fa fa-external-link"></i>
                                    </span>
                                </a>
                            }
                            @if (Model.Buttons[1].TextButton != "")
                            {
                                <a href="@Model.Buttons[1].UrlButton" target="_blank" class="fancy-button button-line button-white zoom @Model.Buttons[1].StButton">
                                    @Html.Raw(Model.Buttons[1].TextButton)
                                    <span class="icon">
                                        <i class="fa fa-external-link"></i>
                                    </span>
                                </a>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--=== Home Section Ends ===-->
    </div>


    <!--=== Features section Starts ===-->
    @if (Model.ItemService.ItemActive)
    {
        <section id="section-feature" class="feature-wrap">
            <div class="container features center">
                <div class="row">
                    <div class="col-lg-12">
                        <!--Features container Starts -->
                        <ul id="card-ul" class="features-hold baraja-container">
                            @foreach (var item in Model.ItemService.ListItens)
                            {
                                <!-- Single Feature Starts -->
                                <li class="single-feature" title="@item.Title">
                                    <img src="~/Content/uploads/@item.UserImageGallery.Folder/@item.UserImageGallery.FileName" alt="" class="feature-image" />
                                    <h4 class="feature-title color-scheme @item.ComponentServiceOption.Title">@Html.Raw(item.Title)</h4>
                                    <p class="feature-text @item.ComponentServiceOption.Description">
                                        @Html.Raw(item.Description)
                                    </p>
                                </li>
                            }
                        </ul>
                        <!-- Features Controls Starts -->
                        <div class="features-control relative">
                            <button class="control-icon fancy-button button-line no-text btn-col bell" id="feature-prev" title="Previous">
                                <span class="icon">
                                    <i class="fa fa-arrow-left"></i>
                                </span>
                            </button>
                            <button class="control-icon fancy-button button-line no-text btn-col zoom" id="feature-expand" title="Expand">
                                <span class="icon">
                                    <i class="fa fa-expand"></i>
                                </span>
                            </button>
                            <button class="control-icon fancy-button button-line no-text btn-col zoom" id="feature-close" title="Collapse">
                                <span class="icon">
                                    <i class="fa fa-compress"></i>
                                </span>
                            </button>
                            <button class="control-icon fancy-button button-line no-text btn-col bell" id="feature-next" title="Next">
                                <span class="icon">
                                    <i class="fa fa-arrow-right"></i>
                                </span>
                            </button>
                        </div>
                        <!-- Features Controls Ends -->
                    </div>
                </div>
            </div>
        </section>
    }
    <!--=== Features section Ends ===-->
    <!--=== Services section Starts ===-->
    @if (Model.ItemActivity.ItemActive)
    {
        <section id="section-services" class="services-wrap">
            <div class="container services">
                <div class="row">
                    <div class="col-md-10 col-md-offset-1 center section-title">
                        <h3 class="@Model.ItemActivity.ItemStTitle">@Html.Raw(Model.ItemActivity.ItemTitle)</h3>
                    </div>
                    @{i = 0; j = 800; cl = "";}
                    @foreach (var item in Model.ItemActivity.ListItens)
                    {
                        if (i % 2 == 0) { cl = "fadeInRight"; } else { cl = "fadeInUp"; }
                        <div class="col-md-6 col-sm-6 service animated" data-animation="@cl" data-animation-delay="@j">
                            <span class="service-icon center"><i class="@item.VectorIcon fa-3x"></i></span>
                            <div class="service-desc">
                                <h4 class="service-title color-scheme @item.ComponentActivityOption.Title">@Html.Raw(item.Title)</h4>
                                <p class="service-description justify @item.ComponentActivityOption.Description">
                                    @Html.Raw(item.Description)
                                </p>
                            </div>
                        </div>
                        i++; j = j + 800;
                    }
                </div>
            </div>
        </section>
    }
    <!--=== Services section Ends ===-->
    <!--=== Step-1 section Starts ===-->
    <section id="step-1" class="section-step step-wrap">
        <div class="container step animated" data-animation="bounceInLeft" data-animation-delay="700">
            <div class="row">
                <!-- Step Description Starts -->
                <div class="col-md-8 step-desc">
                    <div class="col-md-10 step-details">
                        <h3 class="step-title color-scheme @Model.Textos[2].StText">@Html.Raw(Model.Textos[2].Text)</h3>
                        <p class="step-description @Model.Textos[3].StText">@Html.Raw(Model.Textos[3].Text)</p>
                    </div> <!-- End step-details -->
                </div>
                <!-- Step Description Ends -->
                <div class="col-md-4 step-img">
                    <img src="~/Content/uploads/@Model.ImagensForm[3]" alt="" /> <!-- Step Photo Here -->
                </div>
            </div>
        </div>
    </section>
    <!--=== Step-1 section Ends ===-->
    <!--=== Step-2 section Starts ===-->
    <section id="step-2" class="section-step step-even step-wrap">
        <div class="container step animated" data-animation="bounceInRight" data-animation-delay="700">
            <div class="row">
                <!-- Step Description Starts -->
                <div class="col-md-8 step-desc">
                    <div class="col-md-10 step-details">
                        <h3 class="step-title color-scheme @Model.Textos[4].StText">@Html.Raw(Model.Textos[4].Text)</h3>
                        <p class="step-description @Model.Textos[5].StText">@Html.Raw(Model.Textos[5].Text)</p>
                    </div> <!-- End step-details -->
                </div>
                <!-- Step Description Ends -->
                <div class="col-md-4 step-img">
                    <img src="~/Content/uploads/@Model.ImagensForm[4]" alt="" /> <!-- Step Photo Here -->
                </div>
            </div>
        </div>
    </section>
    <!--=== Step-2 section Ends ===-->
    <!--=== Video section Starts ===-->
    <section id="section-video" class="section-video-wrap">
        <div class="section-overlay"></div>
        <div class="container big-video center animated" data-animation="fadeInLeft" data-animation-delay="700">
            <div class="row">
                <div class="col-md-12 section-title">
                    <h3 class="@Model.Textos[6].StText">@Html.Raw(Model.Textos[6].Text)</h3>
                </div>
                <div class="col-md-10 col-md-offset-1 video-content">
                    <iframe src="@Model.Videos[0]" width="400" height="240"></iframe>
                </div>
            </div>
        </div>
    </section>
    <!--=== Video section Ends ===-->
    <!--=== ScreenShots section Starts ===-->
    @if (Model.ItemPortofolio.ItemActive)
    {
        <section id="section-screenshots" class="screenshots-wrap">
            <div class="container screenshots animated" data-animation="fadeInUp" data-animation-delay="1000">
                <div class="row porfolio-container">
                    <div class="col-md-10 col-md-offset-1 center section-title">
                        <h3 class="@Model.ItemPortofolio.ItemStTitle">@Html.Raw(Model.ItemPortofolio.ItemTitle)</h3>
                    </div>
                    @foreach (var item in Model.ItemPortofolio.ListItens)
                    {
                        <div class="col-md-4 col-sm-4 col-xs-6">
                            <div class="screenshot">
                                <div class="photo-box">
                                    <img src="~/Content/uploads/@item.UserImageGallery.Folder/@item.UserImageGallery.FileName" alt="" />
                                    <div class="photo-overlay">
                                        <h4 class="@item.ComponentPortofolioOption.Title">@Html.Raw(item.Title)</h4>
                                    </div>
                                    <span class="photo-zoom">
                                        <a href="@Url.Action("SingleProject", "Egret", new { title = item.Title, description = item.Description, category = item.Category, imgFolder = item.UserImageGallery.Folder, imgFileName = item.UserImageGallery.FileName })" class="view-project"><i class="fa fa-search-plus fa-2x"></i></a>
                                    </span>
                                </div>
                            </div>
                        </div>
                    }
                </div>

                <div id="portfolio-loader" class="center">
                    <div class="loading-circle fa-spin"></div>
                </div> <!--=== Portfolio loader ===-->

                <div id="portfolio-load"></div><!--=== ajax content will be loaded here ===-->

                <div class="col-md-12 center back-button">
                    <a class="backToProject fancy-button button-line bell btn-col" href="">
                        Voltar
                        <span class="icon">
                            <i class="fa fa-arrow-left"></i>
                        </span>
                    </a>
                </div><!--=== Single portfolio back button ===-->
            </div>
        </section>
    }

    @if (Model.ItemClient.ItemActive)
    {
        <section id="section-testimonials" class="testimonials-wrap">
            <div class="section-overlay"></div>
            <div class="container testimonials center animated" data-animation="rollIn" data-animation-delay="500">
                <div class="row">
                    <div class="col-md-8 col-md-offset-2">
                        <div class="testimonial-slider">
                            @foreach (var item in Model.ItemClient.ListItens)
                            {
                                <div class="testimonial">
                                    <p class="comment @item.ComponentClientOption.Comment">
                                        @Html.Raw(item.Comment)
                                    </p>

                                    <h5 class="happy-client @item.ComponentClientOption.Name">@Html.Raw(item.Name)</h5>
                                    <span class="client-info @item.ComponentClientOption.Functio">@Html.Raw(item.Functio)</span>
                                </div>
                            }
                        </div>
                        <div id="bx-pager" class="client-photos">
                            @{i = 0;}
                            @foreach (var item in Model.ItemClient.ListItens)
                            {
                                <a data-slide-index="@i" href="" class="photo-hold">
                                    <span class="photo-bg">
                                        <img src="~/Content/uploads/@item.UserImageGallery.Folder/@item.UserImageGallery.FileName" alt="" />
                                    </span>
                                </a>
                                i++;
                            }
                        </div>
                    </div>
                </div>
            </div>
        </section>
    }

    <!--=== Pricing section Starts ===-->
    @if (Model.ItemPricing.ItemActive)
    {
        <section id="section-pricing" class="pricing-wrap">
            <div class="container pricing">
                <div class="row">
                    <div class="col-md-10 col-md-offset-1 center section-title">
                        <h3 class="@Model.ItemPricing.ItemStTitle">@Html.Raw(Model.ItemPricing.ItemTitle)</h3>
                    </div>
                    @{j = 700;}
                    @foreach (var item in Model.ItemPricing.ListItens)
                    {
                        cl = item.Destacar ? "best-pricing" : "";
                        <div class="col-md-3 col-sm-6 single-pricing-wrap center animated" data-animation="bounceInLeft" data-animation-delay="@j">
                            <div class="single-pricing @cl">

                                <div class="pricing-head">
                                    <h4 class="pricing-heading color-scheme">@Html.Raw(item.NomePlano)</h4>
                                    <div class="price">
                                        <h3>
                                            <span class="dollar">@item.Moeda</span>
                                            @item.PriceUnid
                                            <span class="month">/@Html.Raw(item.Periodo)</span>
                                        </h3>
                                    </div>
                                </div>
                                <ul class="package-features">
                                    <li>@Html.Raw(item.Description)</li>
                                </ul>
                                <div class="sign-up">
                                    <a href="@item.UrlButton" class="fancy-button button-line btn-col vertical">
                                        @item.TextButton
                                        <span class="icon">
                                            <i class="fa fa-hand-o-up"></i>
                                        </span>
                                    </a>
                                </div>
                            </div>
                        </div>
                        j = j + 300;
                    }
                </div>
            </div>
        </section>
    }
    <!--=== Pricing section Ends ===-->
    <!--=== Contact section Starts ===-->
    <section id="section-contact" class="contact-wrap">
        <div class="section-overlay"></div>
        <div class="container contact center animated" data-animation="flipInY" data-animation-delay="1000">
            <div class="row">
                <div class="col-md-8 col-md-offset-2">
                    <div class="row">
                        <div class="col-md-10 col-md-offset-1 center section-title">
                            <h3 class="@Model.Textos[7].StText">@Html.Raw(Model.Textos[7].Text)</h3>
                        </div>
                    </div>
                    <div class="row">
                        <form class="contact-form support-form" id="main-contact-form" name="contact-form" method="post" action="@Url.Action("EmailS","AppServices")">
                            <input type="hidden" name="siteNumber" value="@Model.Profile.SiteNumber" />
                            <div class="col-lg-12">
                                <input id="name" class="input-field form-item field-name" type="text" required="required" name="name" placeholder="Seu Nome" />
                                <input id="email" class="input-field form-item field-email" type="email" required="required" name="email" placeholder="Seu Email" />
                                <input id="subject" class="input-field form-item field-subject" type="text" required="required" name="subject" placeholder="Assunto" />
                                <textarea id="message" class="textarea-field form-item field-message" rows="5" name="message" placeholder="Mensagem"></textarea>
                            </div>
                            <button type="submit" class="fancy-button button-line button-white large zoom subform">
                                Enviar Email
                                <span class="icon">
                                    <i class="fa fa-paper-plane-o"></i>
                                </span>
                            </button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--=== Contact section Ends ===-->
    <!--=== Footer section Starts ===-->
    <div id="section-footer" class="footer-wrap">
        <div class="container footer center">
            <div class="row">
                <div class="col-lg-12">
                    <h4 class="footer-title">
                        <!-- Footer Title -->
                        <a class="site-name">@Model.Profile.Empresa</a>
                    </h4>
                    <h6>@Model.Profile.Adress</h6>
                    <h6>Tel: @Model.Profile.Phone</h6>
                    @if (Model.Profile.WhatsApp != "")
                    {
                        <h6>WhatsApp: @Model.Profile.WhatsApp</h6>
                    }
                    <h6>Email: @Model.Profile.Email</h6>
                    @if (Model.Profile.Website != "")
                    {
                        <h6>WebSite: <a href="@Model.Profile.Website" target="_blank">@Model.Profile.Website</a></h6>
                    }
                    <div class="social-icons">
                        @if (Model.ItemSocialNetwork.ItemActive)
                        {
                            if (Model.ItemSocialNetwork.ListItens.Count() > 0)
                            {
                                <ul>
                                    @foreach (var item in Model.ItemSocialNetwork.ListItens)
                                    {
                                        <li><a href="@item.Link" target="_blank"><i class="@item.Classe1"></i></a></li>
                                    }
                                </ul>
                            }
                            else
                            {
                                <ul>
                                    <li><a class="popup" href="https://www.facebook.com/sharer/sharer.php?u="><i class="fa fa-facebook-square"></i></a></li>
                                    <li><a class="popup" href="http://twitter.com/share?text=Vejam%20esta%20página&url="><i class="fa fa-twitter-square"></i></a></li>
                                    <li><a class="popup" href="https://plus.google.com/share?url="><i class="fa fa-google-plus-square"></i></a></li>
                                    <li><a class="popup" href="http://www.linkedin.com/shareArticle?mini=true&amp;url="><i class="fa fa-linkedin-square"></i></a></li>
                                    <li><a class="popup" href="http://www.tumblr.com/share/link?url="><i class="fa fa-tumblr-square"></i></a></li>
                                </ul>
                            }
                        }
                    </div>
                    <br />
                    <p class="copyright">All rights reserved &copy; 2015</p>
                </div>
            </div>
        </div>
    </div>
    <!--=== Footer section Ends ===-->
    <!--==== Js files ====-->
    <!--==== Essential files ====-->
    <script type="text/javascript" src="~/Componentes/Professional/Egret/js/jquery-1.11.1.min.js"></script>
    <script type="text/javascript" src="~/Componentes/Professional/Egret/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Componentes/Professional/Egret/js/bootstrapValidator.min.js"></script>
    <script type="text/javascript" src="~/Componentes/Professional/Egret/js/modernizr.js"></script>
    <script type="text/javascript" src="~/Componentes/Professional/Egret/js/jquery.easing.1.3.js"></script>

    <!--==== Slider and Card style plugin ====-->
    <script type="text/javascript" src="~/Componentes/Professional/Egret/js/jquery.baraja.js"></script>
    <script type="text/javascript" src="~/Componentes/Professional/Egret/js/jquery.vegas.min.js"></script>
    <script type="text/javascript" src="~/Componentes/Professional/Egret/js/jquery.bxslider.min.js"></script>

    <!--==== MailChimp Widget plugin ====-->
    <script type="text/javascript" src="~/Componentes/Professional/Egret/js/jquery.ajaxchimp.min.js"></script>

    <!--==== Scroll and navigation plugins ====-->
    <script type="text/javascript" src="~/Componentes/Professional/Egret/js/jquery.nicescroll.min.js"></script>
    <script type="text/javascript" src="~/Componentes/Professional/Egret/js/jquery.nav.js"></script>
    <script type="text/javascript" src="~/Componentes/Professional/Egret/js/jquery.appear.js"></script>
    <script type="text/javascript" src="~/Componentes/Professional/Egret/js/jquery.fitvids.js"></script>

    <!--==== Custom Script files ====-->
    <script type="text/javascript" src="~/Componentes/Professional/Egret/js/custom.js"></script>
    <script src="~/Scripts/Ishopping/SocialNetworks.min.js"></script>

    <script type="text/javascript">
        $(window).ready(function () {
            'use strict';
            $.vegas('slideshow', {
                backgrounds: [
                  { src: '../../Content/uploads/@Model.ImagensForm[0]', fade: 1000 },
                  { src: '../../Content/uploads/@Model.ImagensForm[1]', fade: 1000 },
                  { src: '../../Content/uploads/@Model.ImagensForm[2]', fade: 1000 }
                ]
            })();
        });
    </script>

</body>
</html>