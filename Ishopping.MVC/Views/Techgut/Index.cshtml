                            <p class="footer-heading">link</p>
                            <ul class="footermenu">
                                @foreach (var menu in Model.Menu)
                                {
                                    foreach (var item in menu.UserMenuViewItem.Where(x => x.OnMenu == true))
                                    {
                                        <li><a data-scroll data-options='{ "easing": "easeInQuad" }' href="@item.Link">@item.TextMenu</a></li>
                                    }
                                }
                            </ul>
                        </div><!-- /.col-xs-12 .col-sm-3 .col-md-3 -->
                        <div class="wow zoomIn col-xs-12 col-sm-6 col-md-6">
