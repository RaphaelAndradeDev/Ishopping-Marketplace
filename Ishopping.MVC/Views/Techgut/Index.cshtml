                                @if (Model.ItemSocialNetwork.ItemActive)
                                {
                                    if (Model.ItemSocialNetwork.ListItens.Count() > 0)
                                    {
                                        foreach (var item in Model.ItemSocialNetwork.ListItens)
                                        {
                                            <li><a href="@item.Link" target="_blank"><i class="@item.Classe1"></i></a></li>
                                        }
                                    }
                                    else
                                    {
                                        <li><a class="popup" href="https://www.facebook.com/sharer/sharer.php?u=" target="_blank"><i class="fa fa-facebook"></i></a></li>
                                        <li><a class="popup" href="http://twitter.com/share?text=Vejam%20esta%20página&url=" target="_blank"><i class="fa fa-twitter"></i></a></li>
