                                {
                                    <li><i class="fa fa-whatsapp"></i><span>WhatsApp: </span><a> @Model.Profile.WhatsApp</a></li>
                                }
                                <li><i class="flaticon-email21"></i><span>E-Mail:</span><a> @Model.Profile.Email</a></li>
                                @if (Model.Profile.Website != "")
                                {
                                    <li><i class="flaticon-world91"></i><span>Website:</span><a href="@Model.Profile.Website" target="_blank"> @Model.Profile.Website</a></li>
                                }
                            </ul>
                            <i class="flaticon-home78"></i>
                        </div><!-- /.col-xs-12 .col-sm-3 .col-md-3 -->
                    </div> <!-- /.row -->
                </div> <!-- /.container -->
