@model List<Ishopping.MVC.ViewModels.User.UserImageGalleryViewModel>
@{
    ViewBag.Title = "Imagens Componente Apresentação";
    Layout = "~/Views/Shared/_LayoutImage.cshtml";
}

<input type="hidden" id="data" data-imgtype="Apresentação" data-imgsearch="PresentationSearchShare" data-imgcontroller="GetImgPresentation" />

@Html.Partial("_ImageGalleryPartial", new ViewDataDictionary(this.ViewData) { { "fileType", "15" } })

<div class="row">
    @foreach (var item in Model)
    {
        @Html.Partial("_PortfolioPartialT2I3", item)
    }
</div>

@section scripts{
    <script src="~/Scripts/Ishopping/ImageGallery/ImageGalleryT2.min.js"></script>
}





