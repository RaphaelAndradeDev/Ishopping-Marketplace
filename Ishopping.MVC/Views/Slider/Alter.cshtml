            </div>
            <div class="input-group mgbt col-xs-12 col-sm-3 col-md-2 col-lg-2">
                <span class="input-group-addon">Alt</span>
                <input type="number" class="form-control imgconfig" id="DataImgHeight" name="DataImgHeight">
            </div>
            <div class="input-group mgbt col-xs-12 col-sm-3 col-md-2 col-lg-2">
                <span class="input-group-addon">Z</span>
                <input type="number" class="form-control imgconfig" id="DataImgZindex" name="DataImgZindex">
            </div>
        </div>
        <br />
        <h4 class="color1">Texto :</h4>
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-10 col-lg-8">
                <div class="input-group">
                    <input class="form-control txt" type="text" id="Text" name="Text" placeholder="Título" />
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-leaf bntStyle"></span>
                    </span>
                </div>
                @Html.Partial("_IsTagsPartial")
                <small class="text-muted">Estilo: <span id="sttext"></span></small>
            </div>
        </div>
        <h4 class="color1">URL :</h4>
        <div class="form-group">
            <input class="form-control txt" id="Url" type="text" name="Url" placeholder="http://algumsite.com" />
        </div>
        <br />
        <div class="row">
            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-2">
                <h4 class=" color1">Nome da imagem :</h4>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-5 col-lg-4">
                <div class="form-group">
                    <input class="form-control texto txt" type="text" id="ImageFileName" name="ImageFileName" placeholder="Nome da imagem" />
                </div>
            </div>
            <div class="col-md-4">
                <button type="button" class="btn btn-primary" id="goToGallery" style="margin:2px">Ir para Galeria</button>
            </div>
        </div>
        <p>
            <div id="Info"></div>
            <div class="form-group">
                <button type="button" class="btn btn-primary" id="novo" style="margin:2px">Novo</button>
                <button type="submit" class="btn btn-primary" id="salvar" style="margin:2px">Salvar</button>
                <button type="button" class="btn btn-danger" id="deletar" style="margin:2px">Deletar</button>
            </div>
        </p>
        <text id="Status"></text>
    </div>
</form>

@section scripts{
    <script src="~/Scripts/Ishopping/Content/ContentSlider.min.js"></script>
}












